{
  "name": "agora-rtc-vue",
  "version": "0.2.54",
  "private": false,
  "author": "anzhaowei",
  "scripts": {
    "serve": "cross-env vue-cli-service serve --https",
    "build": "vue-cli-service build",
    "test:serve": "cross-env NODE_ENV=test vue-cli-service test:unit --watch",
    "test:unit": "cross-env NODE_ENV=test vue-cli-service test:unit",
    "lint": "vue-cli-service lint --fix",
    "api": "api-extractor run --verbose && rimraf ./lib/temp && rimraf ./lib/types/tsdoc-metadata.json",
    "init:install": "node ./install_dependencies.js",
    "docs:serve": "yarn lib:serve && yarn docs:dev & yarn keep-building",
    "keep-building": "nodemon ./building.js --watch src --watch packages -e vue,js,json,ts,tsx,jsx --delay 2",
    "lib": "vue-cli-service build --target lib --name agora-rtc-vue --dest lib packages/index.ts --mode production",
    "lib:build": "yarn lib && rimraf ./lib/*.html && yarn api",
    "lib:publish": "yarn lib:build && yarn publish",
    "lib:report": "vue-cli-service build --target lib --name agora-rtc-vue --dest lib packages/index.ts --mode production --report",
    "lib:serve": "vue-cli-service build --target lib --name agora-rtc-vue --dest lib packages/index.ts --mode development"
  },
  "main": "lib/agora-rtc-vue.umd.min.js",
  "typings": "lib/types/AgoraRtcVue.d.ts",
  "files": [
    "lib/**/*.(js|css)",
    "lib/types/*.d.ts"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.12.1",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^8.2.2",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "agora-rtc-sdk-ng": "^4.5.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "istanbul-instrumenter-loader": "^3.0.1",
    "karma-coverage": "^2.0.3",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-spec-reporter": "^0.0.32",
    "karma-typescript": "^5.5.1",
    "lint-staged": "^11.0.0",
    "mocha": "^6.2.0",
    "nodemon": "^2.0.6",
    "prettier": "^1.19.1",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "typescript": "~3.9.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "vue": "^2.6.12",
    "vue-cli-plugin-unit-karmajs": "~1.0.1",
    "vue-template-compiler": "^2.6.12",
    "webpack-replace": "^1.0.0"
  },
  "peerDependencies": {
    "agora-rtc-sdk-ng": "^4.5.0",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "declaration": true,
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,vue}": [
      "vue-cli-service lint --fix",
      "git add"
    ]
  }
}
